         left: 0;
         width: auto
         top: 20px;
   &__description
      padding-top: 20px;
      padding-bottom: 15px;
      margin-left: 15px;
      margin-right: 15px;
      border-bottom: 1px solid #e9ebee;
      @media(max-width: 767px)
         padding-top: 15px;
   &__price-block
      display: flex;
      justify-content: center;
      align-items: center;
   &__price
      font-size: 24px;
      padding: 20px 5px;
      text-align: center;
      color: #000000
      white-space: nowrap;
      @media(max-width: 767px)
         padding: 0;
         margin-left: 10px;
      @media(max-width: $sm)
         font-size: 21px;
   &__old-price
      text-decoration: line-through   
      font-size: 20px;   
      padding: 20px 5px;
      white-space: nowrap;
   &__item
      margin-bottom: 10px;
      @media(max-width: 767px)
         margin-bottom: 5px;
   &__close
      position: absolute;
      right: 10px;
      top: 20px;
      display: none;
      @media(max-width: 767px)
         display: block
   &__img
      width:100%
      @media(max-width: 767px)
         display: none;
   &__text
      font-size: 14px;
      padding: 0 10px 25px 10px;
      text-align:center
      @media(max-width: 767px)
         padding-bottom: 0;
.gallery-modal
   display: flex;
