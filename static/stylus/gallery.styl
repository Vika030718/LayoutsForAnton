   &__head_col
      margin-left: 430px;
      @media(max-width: 945px)
         margin-left: 0;
   &__bottom
      display: flex;
      align-items: center;
      padding: 30px 0 20px 0;
      @media(max-width: $md)
         flex-direction: column;
         padding-top: 10px;
   &__bottom_col
      flex-direction: column;
      justify-content: flex-start
      align-items: flex-start;
   &__link
      font-size: 16px;
      text-decoration: underline
   &__description
      color: rgba(85, 90, 105, 0.75);
      font-size: 14px;
      padding-top: 15px;
      padding-bottom: 25px;
   &__title
      font-size: 24px;
      @media(max-width: 792px)
         width: 100%;
         margin-top: 20px;
         text-align: left;
   &__main-arrow
      position: relative;
      top: 50%; 
      color: #adaeaf;
      margin-top: -14px;
      height: 28px;
      &:hover, &:focus
         text-decoration: none!important;
      &:before
         font-size: 16px;
         content: '';
         color: #b4b5b6;
         width: 28px;
         height: 28px;
         border-radius: 28px;
         border: 2px solid #b4b5b6;
         display: block;
         background-color: rgba(255, 255, 255, 0.2);
      &:after
         font-size: 16px;
         color: #B4B5B6;
         top: 50%;
         left: 50%;
         margin-left: -2px;
         margin-top: -9px;
   &__main-arrow_left
      &:after
         content: "\f104";
         font-family: fontAwesome;
         position: absolute;
         left: 50%;
   &__main-arrow_right
      margin-left: 10px;
      @media(min-width: $lg) and (max-width: 1170px)
         right: 0;
      @media(max-width: $sm)
         right: 0;
      &:after
         content: "\f105";
         font-family: fontAwesome;
         position: absolute;
         left: 50%;
   &__main-arrow_notactive
      &:before
         border: 2px solid #e4e4e4;
      &:after
         color: #e4e4e4;
   &__controls
      display: flex;
   &__img
      margin-right: 25px;
