.compare-item
   padding-bottom: 30px;
   @media(max-width: $lg)
      display: none;
      width: 100%;
   &_first
      display: block;
   &__img-block
      margin-bottom: 15px;
      display: block;
      overflow: hidden;
      position: relative
      height: 400px;
      margin-left: 15px;
      margin-right: 15px;
      padding-top: 25px;
      max-width: 300px;
      @media(max-width: $lg)
         margin-left:auto;
         margin-right:auto;
      @media(max-width: $md)
         margin: auto;
         height: auto!important;
         margin-bottom: 15px;
         margin-left:auto;
         margin-right:auto;
   &__title
      font-size: 13px;
      margin-bottom: 5px;
      margin-left: 10px;
      @media(max-width: $md)
         margin-left: auto;
   &__price
      font-size: 13px; 
      margin-left: 10px; 
      margin-bottom: 17px;
   &_centered
      border-left: 1px solid #ebedf1;
      border-right: 1px solid #ebedf1;
   &__info
      padding: 0 10px 0 10px;
      .options
         margin-bottom: 20px;
      .table__cell
         color: #555a69;
         vertical-align: middle;
      .scale__item_active
         border-right: 1px solid #dadada;
         border-left: 1px solid #dadada;
      .scale__item:nth-child(1)
         border-top-left-radius: 2px;
         border-bottom-left-radius: 2px;
      .scale__item:nth-last-child(1)
         border-top-right-radius: 2px;
         border-bottom-right-radius: 2px;
   &__btn
      margin-bottom: 15px;
   &__footer
      padding:0 5px;
   &__remove
      position: absolute;  
      top:43px;    
      width:50px;
      border-top-right-radius: 50px;
      border-bottom-right-radius: 50px;
      background-color:rgba(0, 0, 0, 0.80);
      height: 45px;
      display: flex;
      justify-content: center;
      align-items: center;
   &__link
      text-transform: uppercase
      color: inherit
      display: block
      min-width: 100px;
      @media(max-width: $md)
         padding: 10px 0;
         display: block;
      &:after
         content: url("../img/product__close.png");
         right: 0;
         position: absolute;
.gallery-item
   margin-bottom: 20px;
   border: 2px solid transparent
   padding-bottom: 20px;
   @media(max-width: $lg)
      text-align: center;
      margin-bottom: 20px;
      max-width: 224px;
      margin-left: auto
      margin-right: auto
   &:hover
      border-color: #000000!important
      background-color: #ffffff;
      position: relative
      max-width: 224px;
      margin-left: auto;
      flex: 0 1 auto;
      margin-right: auto;
      cursor: pointer
      .gallery-item__link
         opacity: 1; 
   &_active
      border-color: #000000!important
      background-color: #ffffff;
      position: relative
      max-width: 224px;
      margin-left: auto;
      flex: 0 1 auto;
      margin-right: auto;
      &:after
         content: "\f107"
         width:35px;
         height: 35px;
         border-radius: 34px;
         background-color: #000000
         font-family: fontAwesome
         position: absolute;
         right: -17px;
         top: -17px;
         color: #ffffff
         font-size: 29px;
         line-height: 35px;
         text-align: center;
      .gallery-item__link
         opacity: 1;
   &__img-block
      max-width: 220px;
      margin-bottom: 15px;
      display: block;
      overflow: hidden;
      position: relative
      height: 370px;
      margin-left: auto;
      margin-right: auto;
      @media(max-width: $md)
         margin: auto;
         height: auto!important;
         max-width: none;
         margin-bottom: 15px;
   &__img-block_button
      height: 150px
   &__img-block_square
      height: 280px;
   &__img-block_side
      height: 220px;
      @media(max-width: $md)
         height: auto;
   &__img-block-cloth
      height: 280px;
      background-color: transparent
   &__img-block_col
      height: 200px;
      .gallery-item__img
         margin-left: -100px;
         @media(max-width: $md)
            margin-left: auto;
   &__img
      display: block;
      margin: auto;
      position: absolute;
      left: 50%;
      margin-left: -110px;
      // margin-left: -50%;
      // max-width: 220px;
      // width: 100%;
      // width: auto;
      // max-width: 230px;
      // margin-left: -50%;
      height: 100%;
      @media(max-width: $md)
         width: 100%;
         position: static;
         margin-left: auto;
         // max-width: 469px;
   &__img-cloth
      width: 100%;
      margin-left: -50%;
      @media(max-width: $md)
         width: auto;
         margin-left: auto;
   &__img_button
      margin-left: -75px;
      @media(max-width: $md)
         width: 100%;
         margin-left: auto;
   &__title
      font-size: 13px;
      margin-bottom: 5px;
      margin-left: 10px;
      @media(max-width: $md)
         margin-left: auto;
   &__title-price
      color: #555a69
      font-size: 13px;
      margin-bottom: 5px;
      margin-left: 10px;
   &__subtitle
      color: $text-pale;
      font-size: 13px;
      margin-bottom: 5px;
   &__price-block
      display: flex;
      align-items: center;
      @media(max-width: $lg)
         justify-content: center;
   &__price
      font-size: 13px; 
      margin-left: 10px; 
   &__price-old
      text-decoration: line-through;
      font-size: 15px; 
      font-size: 13px; 
      margin-left: 10px; 
   &__filter-block
      display: flex
   &__size
      font-size: 13px;
      color: #555a69
      margin-left: 10px;
      margin-bottom: 5px;
   &__link
      margin-left: 10px;
      font-size: 13px;
      text-decoration: underline
      margin-bottom: 15px;
      display: block;
      opacity:0;
      margin-top: 15px;
   &__options
      background-color: #ffffff
      display: flex;
      flex-direction: column;
      padding: 30px 40px 30px 35px;
      height: 100%;
      text-align: left;
      @media(max-width: $sm)
         padding: 20px 10px 20px 20px;
      .filter__label
         font-size: 13px;
         padding-left: 23px;
   &__options-title
      margin-bottom: 30px;
      font-size: 15px;
.gallery
   background-color: #f5f6f8;
   @media(min-width: $md)
      border-top: 1px solid $separator-color;
   &__select
      margin-right: 40px;
      position: relative;
      min-width: 210px;
      background-color: #fafbfc
      @media(max-width: $lg)
         margin-right: 20px;
         min-width: 180px;
      @media(max-width: $sm)
         margin-right: 0;
         margin-bottom: 10px;
      select
         font-size: 13px;
      &:after
         top: 11px;
   &__select_inactive
      select
         border-color: #cccccc;
         color: #cccccc
      &:after
         color: #cccccc
   &__submenu-col
      border-bottom: 1px solid #e9ebee;
   &__submenu
      display: flex;
      @media(max-width: $md)
         display: block
   &__submenu-filter
      justify-content: flex-end;
      .filter__color-label
         border-radius: 2px;
         box-shadow: inset 0 0 2px #050505;
      @media(max-width: $md)
         justify-content: center;
   &__submenu-item
      display: flex;
      align-items: center;
   &__title
      font-size: 24px;
      padding : 25px 0px 0;
      border-top: 1px solid $separator-color;
   &__block-head
      position: relative
      display: flex;
      font-size: 24px;
      margin-bottom: 30px;
      cursor: pointer
      &:after
         font-family: fontAwesome;
         font-size: 38px;
         position: absolute;
         top: 50%;
         margin-top: -10px;
         right: 20px;
         width: 30px;
         height: 20px;
         margin-top: -11px;
         content: '\f107';
   &__block-head_active
      &:after
         content: '\f106';
   &__block-number
      padding: 20px 25px;
      background-color: #e9ebee;
      display: flex;
      align-items: center;
      color: #300089;
   &__block-name
      padding: 17px 0 17px 25px;
      font-size: 24px;
      background-color: #e9ebee;
      width: 100%
      @media(max-width: $sm)
         font-size: 18px;
         display: flex;
         align-items: center;
   &__block-number
      border-right: 1px solid #ffffff
   &__block-title
      font-size: 24px;
      padding : 15px 0px 20px;
      text-align: center
   &__block
      border-bottom: 1px solid #e9ebee;
      padding-bottom:25px;
   &__block-title-name
      padding-bottom: 30px;
   &__subtitle
      font-size: 24px;
      text-align: center
      padding: 20px 0
      border-bottom: 1px solid #e9ebee;
      width: 100%;
   &__subtitle-link
      text-decoration: underline
      color: #555a69;
      &:hover
         color: #000000
   &__subtitle-col
      font-size: 15px;
      color: #555a69
      padding: 20px 0 30px 0;
      text-align: center;
   &__subtext
      color: #555a69
      text-align: center
      font-size: 15px;
      padding: 20px 0 35px 0;
   &__title-top
      border-top: none;
   &__main-arrow
      position: relative;
      top: 50%; 
      color: #adaeaf;
      margin-top: -22px;
      height: 45px;
      line-height: 16px;
      &:hover, &:focus
         text-decoration: none!important;
      &:before
         font-size: 40px;
         content: '';
         color: #b4b5b6;
         width: 45px;
         height: 45px;
         border-radius: 45px;
         border: 2px solid #b4b5b6;
         display: block;
         background-color: rgba(255, 255, 255, 0.2);
      &:after
         font-size: 22px;
         color: #B4B5B6;
         top: 50%;
         left: 50%;
         margin-left: -5px;
         margin-top: -9px;
   &__main-arrow_left
      &:after
         content: "\f104";
         font-family: fontAwesome;
         position: absolute;
         left: 50%;
   &__main-arrow_right
      margin-left: 10px;
      @media(min-width: $lg) and (max-width: 1170px)
         right: 0;
      @media(max-width: $sm)
         right: 0;
      &:after
         content: "\f105";
         font-family: fontAwesome;
         position: absolute;
         left: 50%;
   &__main-arrow_notactive
      &:before
         border: 2px solid #e4e4e4;
      &:after
         color: #e4e4e4;
   &__filter
      font-size: 14px;
      color: #555a69
      display: flex
      align-items: center;
      @media(max-width: $md)
         padding: 25px 0;
   &__filter-icon
      margin-left: 10px;
   &__filter-number
      color: #300089
   &__nav-btn
      margin-bottom: 15px;
      @media(max-width: 767px)
         display: none!important;
   &__nav-btn-wrapper
      display: flex;
      justify-content: center;
      align-items: flex-end;
   &__head
      display: flex;
      @media(max-width: 767px)
         display: block 
   &__row
      display: flex;
      flex-wrap: wrap;
      &:before,
      &:after
         width:0!important
   &__row_fancywork
      border-bottom: 1px solid #e9ebee;
      padding-bottom: 40px;
      margin-bottom: 30px;
   &__options
      display: flex;
      justify-content: center;
      align-items: center;
      @media(max-width: $sm)
         flex-direction: column;
   &__mesures-title
      font-size: 18px;
      margin-bottom: 15px;
      margin-top: 40px;
      color: #555a69
   &__mesures-subtitle
      font-size: 15px;
      color: #555a69
   &__more
      margin-bottom: 40px;
   &__more-btn
      position: relative
      &:before
         content: "\f107";
         font-family: fontAwesome;
         position: absolute;
         left: 20px;
         top: 50%
         margin-top: -6px;
         font-size: 30px;
      &:after
         content: "\f107";
         font-family: fontAwesome;
         position: absolute;
         right: 20px;
         top: 50%
         margin-top: -6px;
         font-size: 30px;
   &__trim
      .select-color__color-label
         border-radius: 2px;
         box-shadow: inset 0 0 2px #050505;
.gallery-nav
   display: flex;
   justify-content: space-between;
   &__list
      display: flex;
      flex: 0 1 auto;
      overflow: hidden;
      @media(max-width: $xl)
         &:after
            position: absolute;
            bottom: 0;  
            height: 100%;
            width: calc( 100% - 30px);
            content: "";
            background: -moz-linear-gradient(left, rgba(30,87,153,1) 0%, rgba(41,137,216,1) 50%, rgba(32,124,202,1) 51%, rgba(125,185,232,1) 100%); /* FF3.6-15 */
            background: linear-gradient(to left, rgb(245, 246, 248) 20%, rgba(211,20,20,0) 80%);
            pointer-events: none; 
   &__item
      position: relative;
      padding-bottom: 22px;
      padding-top: 22px;
      border-bottom: 3px solid #e9ebee;
      cursor: default!important
      &:after
         content: ""
         background-image: url('../img/gallery-nav-icon.png');
         width: 10px;
         height: 30px;
         display: block;
         position: absolute;
         top: 50%;
         margin-top: -15px;
         right: -10px;
      &:nth-child(1)
         .gallery-nav__link
            padding-left: 0px;
   &__link
      font-size: 16px;
      color: #555a69;
      padding-right: 15px;
      padding-bottom: 21px
      border-bottom:  3px solid transparent;
      transition: color ease-out .2s, border-color ease-out .2s
      padding-left: 27px;
      white-space: nowrap;
      cursor: pointer!important
      text-decoration: none!important;
      &:hover
         text-decoration: none;
         color: #300089
         border-color:  #000000;
         cursor: pointer!important
   &__link_inactive
      &:hover
         text-decoration: none;
         color: #555a69;
         border-color:  transparent;
         cursor: default!important
   &__link_active
      border-bottom:  3px solid #000000;
      color: #300089
      width: 100%;
      height: 1px;
      text-decoration: none;
      cursor: pointer!important
.gallery-nav-btn
   display: flex;
   flex: 1 0 auto;
   border-bottom: 3px solid #e9ebee;
   justify-content: flex-end;
.gallery-breadcrumb
   background-color: transparent;
   padding: 0;
   @media(max-width: $md)
      padding-top: 15px
   &__link
      color: #a1a4ac
      font-size: 14px;
   &__link.active
      color: #555a69
.gallery-advert
   display: flex;
   flex-direction: column;
   justify-content: space-between;
   align-items: center;
   text-align: center;
   height: 88%;
   &__title
      font-size: 24px;
      padding-top: 20%
      @media(max-width: $md)
         font-size: 20px;
         padding-top: 0px
         margin-bottom: 10px;
   &__description
      font-size: 18px;
      font-family: CochinItalic
      max-width: 170px;
      @media(max-width: $md)
         font-size: 17px;
   &__link
      font-size: 20px;
      font-family: CochinItalic
      color: rgba(0, 0, 0, .5);
      text-decoration: underline
      margin-bottom: 10px;
.gallery-actions
   display: flex;
   padding: 20px 0 27px 0;
   justify-content: center;
   @media(max-width: $md)
      padding-bottom: 20px;
   @media(max-width: $sm)
      padding-bottom: 10px;
      padding-top: 10px;
   &__item
      position: relative;
      padding-top: 40px;
      font-size: 12px;
      color: rgba(0, 0, 0, 0.75);
      width: 60px;
      text-align: center;
      @media(max-width: $md)
         padding-top: 35px;
         width: 57px;
      &:before
         content: ""
         background-image: url('../img/gallery/gallery-actions.png');
         width: 31px;
         height: 29px;
         display: block;
         position: absolute;
         top: 0;
         left: 50%;
         margin-left: -15px;
      &:nth-child(1):before
         background-position: 0px 0;
      &:nth-child(2):before
         background-position: 93px 0;
      &:nth-child(3):before
         background-position: 62px 0;
      &:nth-child(4):before
         background-position: 31px 0;
.gallery-panel
   background-color: #ffffff
   font-size: 14px;
   color: #555a69
   display: flex;
   flex-wrap: wrap;
   position: relative
   @media(max-width: 767px)
      position: fixed;
      bottom: 0;
      background-color: #ffffff;
      width: 100%;
      z-index: 2;
      margin: 0 -15px;
      border-top: 1px solid #e9ebee;
   &__body
      width: 100%;
      @media(max-width: 767px)
         display: none;
   &__label
      color: rgba(0, 0, 0, .9);
   &__top
      height:86px;
      overflow: hidden;
      position: relative
      @media(max-width: 767px)
         height: 56px;
         width: 80%;
   &__bottom-item
      flex: 1 1 100%;
      @media(max-width: 767px)
         flex: 1 1 auto;
         order: 1;
         align-items: center;
         display: flex;
         border-top: 1px solid #e9ebee;
         padding: 10px;
         justify-content: center;
   &__bottom-actions
      flex: 1 1 100%;
      @media(max-width: 767px)
         display: none
   &__bottom-arrow
      display: none;
      @media(max-width: 767px)
         display: block
   &__bottom-arrow_open
      transform: rotate(180deg);
   &__title
      font-size: 24px;
      max-width: 180px;
      padding: 14px 0;
      background-color: #ffffff;
      margin: auto;
      text-align: center;
      color: #000000;
      position: relative;
      margin-top: -84px;
      position: absolute;
      left: 50%;
      margin-left: -90px;
      bottom: 0;
      width: 100%;
      @media(max-width: 767px)
         max-width: 100%;
         margin: 0
         text-align: left;
         margin-right: 15px;
         margin-left: 15px;
