/* placeholder 
_______________________________________________*/
.placeholder
	position: relative;
.placeholder label
	font-weight: normal;
	position: absolute;
	top: 0;
	left: 0;
	font-size: 10px;
	padding-left: 14px;
	color: rgba(85, 90, 105, 0.5);
	font-weight: normal;
	transition: all 0.2s;
	touch-action: manipulation;
	font-family: brandonRegular
.placeholder label, .placeholder input, .placeholder textarea
	transition: all 0.2s;
	touch-action: manipulation;
.placeholder input:placeholder-shown + label,
.placeholder textarea:placeholder-shown + label
	cursor: text;
	max-width: 66.66%;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
	transform-origin: left bottom;
	-webkit-transform: translate(0, 2.125rem) scale(1.5);
	-ms-transform: translate(0, 2.125rem) scale(1.5);
	transform: translate(0, 2.125rem) scale(1.5);
	pointer-events: none;
.placeholder ::-webkit-input-placeholder
	opacity: 0;
	transition: inherit;
.placeholder ::-moz-placeholder
	opacity: 0;
	transition: inherit;
// .placeholder :-ms-input-placeholder
// 	opacity: 0;
// 	transition: inherit;
.placeholder input:focus, .placeholder input:enabled,
.placeholder textarea:focus, .placeholder textarea:enabled
	padding: 20px 14px 8px;
.placeholder input:focus::-webkit-input-placeholder,
.placeholder textarea:focus::-webkit-input-placeholder
	opacity: 1;
.placeholder input:focus::-moz-placeholder,
.placeholder textarea:focus::-moz-placeholder
	opacity: 1!important;
.placeholder input:focus:-ms-input-placeholder,
.placeholder textarea:focus:-ms-input-placeholder
	opacity: 1;
.placeholder input:not(:placeholder-shown) + label,
.placeholder input:focus + label,
.placeholder textarea:not(:placeholder-shown) + label,
.placeholder textarea:focus + label,
// .placeholder input:enabled + label,
// .placeholder textarea:enabled + label
	-webkit-transform: translate(0, 5px) scale(1);
	-ms-transform: translate(0, 5px) scale(1);
	transform: translate(0, 5px) scale(1);
	cursor: pointer;
