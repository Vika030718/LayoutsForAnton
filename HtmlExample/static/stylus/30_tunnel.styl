.tunnel
   &__description
      margin-bottom: 20px;
   &__btn
      border: 1px solid #000;
      text-transform: uppercase;
      background-color: #fff;
      color: #000;
      width: 100%;
      padding: 15px;
      font-size: 17px;
      font-family: brandonBold;
      &:hover
         background-color: $hover-main-color;
         color: #fff;
   &__btn-wrapper
      margin-top: 50px;
      margin-bottom: 70px;
   &__confirm
      padding-left: 20px;
      padding-right: 20px;
   &__note
      position: relative;
      font-size: 14px;
      font-family: CochinItalic;
      line-height: 1.2;
      margin-top:30px;
      margin-bottom: 50px;
      padding-left: 30px;
      &:before
         position: absolute;
         content: url('../img/tunnel-arrow.png');
         left: 0px;
         top: -15px;
   &__note-bold
      font-family: cochinBoldItalic;
   &__required
      font-size: 12px;
      display: block;
      max-width: 90px;
      margin-left: auto;
      margin-right: auto;
      margin-bottom: 40px;
   &__checkbox
      font-size: 13px;
   &__address
      padding: 20px;
      font-size: 15px;
      background-image: url('../img/footer_services_bg.jpg');
      line-height: 1.2em;
   &__address-item
      padding: 10px;
   &__address-item_first
      border-right: 1px solid #000;
   &__address-title
      font-size: 20px;
      font-family: brandonBold;
      padding-bottom: 20px;
      font-size: 20px;
      padding-left:10px;
      padding-right:10px;
   &__address-type
      font-size: 15px;
      font-family: brandonBold;
      padding-bottom: 10px;
      font-size: 15px;
   &__address-change
      font-style: italic;
      font-size: 12px;
      text-decoration: underline;
      font-style: italic;
      margin-top: 30px;
      text-align: right;
   &__method-type
      text-transform: uppercase;
      font-family: brandonBold;
      text-transform: uppercase;
      font-size: 15px;
      margin-top: 20px;
   &__method-item
      margin-bottom: 10px;
      font-size: 15px;
      padding-left: 22px;
   &__method-item-time
      color: #999;
      line-height: 1.1em;
      padding-left: 22px;
.payment
   &__card-info
      font-size: 15px;
      font-family: brandonBold;
   &__cards
      padding-bottom: 30px;
      padding-top: 30px;
   &__note
      font-size: 16px;
      line-height: 1.2em;
   &__info
      line-height: 1.2em;
      background-image: url('../img/footer_services_bg.jpg');
      padding: 20px;
      font-size: 15px;
   &__subtitle
      padding-bottom: 20px;
      font-family: brandonBold;
   &__info-text
      padding-bottom: 20px;
   &__btn-wrapper
      margin-top: 30px;
      margin-bottom: 30px;
   &__edit
      font-size: 15px;
      font-family: francoisOne
      text-decoration: underline!important;
   &__form
      padding-top: 20px;
      border-top: 1px solid #000;
      margin-top: 20px;
   &__add
      padding: 6px;
      background-color: #fff;
      border: 1px solid #000;
      color: #000;
   &__currency
      font-family: brandonBold;
      line-height: 34px;
.summary
   &__list
      font-size: 11px;
   &__item
      border-bottom: 1px solid #000;
      padding-top: 25px;
      padding-bottom: 25px;
      line-height: 1.5em;
      display: flex;
   &__item-block
      display: flex;
      flex-direction: column;
      justify-content: center;
   &__img
      max-width: 55px;
   &__title
      font-size: 15px;
      text-transform: uppercase;
      font-family: brandonBold;
   &__description-title
      font-family: brandonBold;
   &__total
      background-image: url('../img/footer_services_bg.jpg');
      padding: 15px;
      font-size: 12px;
   &__price
      padding-right: 15px;
   &__total-price
      font-size: 22px;
      font-family: brandonBold;
   &__total-lable
      font-size: 15px;
      font-family: brandonBold;
      text-transform: uppercase;
